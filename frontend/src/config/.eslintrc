{
    "extends":["react-app","prettier"],
    "plugins": ["react","prettier"],
    "rules": {
        "prettier/prettier": [
            "warn",
            {
                "arrowParents":"always",
                "semi":false,
                "trailingComma": "none",
                "tabWidth": 2,
                "endOfLine": "auto",
                "useTabs": false,
                "singleQuote": true,
                "printWidth": 80,
                "jsxSingleQuote": true
            }
        ]
    }
}